{
    "name": "mez/queuejitsu",
    "description": "A Modern Queueing System",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Meredith",
            "email": "martin@sourceguru.net"
        }
    ],
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": {
            "stickee/*": "source",
            "*": "dist"
        }
    },
    "require": {
        "php": "^7.1",
        "psr/container": "^1.0",
        "psr/log": "^1.0",
        "ramsey/uuid": "^3.7",
        "zendframework/zend-eventmanager": "^3.2"
    },
    "suggest": {
        "predis/predis": "Connect up to Redis",
        "ext-pcntl": "Allows Workers to be ran in forked processes",
        "mez/queuejitsu-cli": "Command Line integration"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "nikic/php-parser": "^3.1",
        "phpmd/phpmd": "^2.6",
        "phpro/grumphp": "^0.11.6",
        "phpstan/phpstan": "^0.8.3 || ^0.9.0",
        "povils/phpmnd": "^1.1",
        "predis/predis": "^1.1",
        "roave/security-advisories": "dev-master",
        "sebastian/phpcpd": "^3.0",
        "sensiolabs/security-checker": "^4.1",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "QueueJitsu\\": "src"
        }
    },
    "extra": {
        "zf": {
            "config-provider": "QueueJitsu\\ConfigProvider"
        }
    }
}
